<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Accounts</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>

    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Seviye Belirleme E-Ticaret II  Nihat Ozan Sandal</p>
        </footer>
    </div>

    <script src="~/App/js/App.Lang.TR.js"></script>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    <script src="~/Scripts/parsley.min.js"></script>
    <script src="~/Scripts/i18n/tr.js"></script>
    <script src="~/App/js/App.Validation.js"></script>
    <script src="~/App/js/App.Security.js"></script>
    <script src="~/App/js/App.FetchData.js"></script>
    <script src="~/App/js/App.FetchDataSucc.js"></script>
    <script src="~/App/js/App.Main.js"></script>

    @RenderSection("myscripts", required: false)

</body>
</html>
